{
  "verbose": true,
  "displayName": "user-service",
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "./helpers/bootstrap-integration-tests.ts"
  ],
  "collectCoverage": true
}